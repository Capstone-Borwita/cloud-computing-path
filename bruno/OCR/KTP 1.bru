meta {
  name: KTP 1
  type: http
  seq: 12
}

post {
  url: {{base_url}}/api/{{api_version}}/ocr/ktp
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{user_token}}
}

body:multipart-form {
  ktp_photo: @file(files/ktp_1.jpg)
}

assert {
  res.status: eq 200
  res.body.data.identifier: isDefined
}

script:post-response {
  bru.setVar('ktp_photo_identifier_1', res.body.data.identifier)
}
